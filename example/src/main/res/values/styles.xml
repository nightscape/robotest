<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="@style/Theme.AppCompat.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="colorPrimary">#CCCCCC</item>
        <item name="colorPrimaryDark">#AAAAAA</item>
    </style>
    <style name="ToolbarCustomIconColor" parent="AppTheme">
        <!-- android:textColorSecondary is used for coloring the navigation drawer icon and up arrow. -->
        <item name="android:textColorSecondary">#FFFFFFFF</item>
    </style>
</resources>
